<!doctype html>
<html lang="en">
  <% include partials/head %>
  <body>
    <% include partials/header %>
    <section>
    <div class="container">
      <% collections.clients.forEach(function(client){ %>
        <article class="clients--container-info">
          <h1><a href=""><%- client.title %></a></h1>
          <h2><%- client.name %></h2>
          <div class="clients--image-container"></div>
          <div>
            <p><%- client.description %></p>
            <a href="#" class="button button-default">
              Learn more about <%- client.name %>
            </a>
          </div>
        </article>
      <% }) %>
    </div>
    </section>
    <% include partials/footer %>
  </body>
</html>
